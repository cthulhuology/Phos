An.object().named('Trash').from(Widget).
	does('init', function() { 
		this.has('trash',an(Image,'images/trash.png').at(Display.width()-64,Display.height()-64).
			has('down', false).
			does('up', function(E) {
				var O = null;
				while(O = E.overlaps([Display,Phosphor,Trash,this])) {
					for (var K in O) {
						console.log(K,O[K])
					}
					
					if (O.visible) { 
						Sound.trash.play();
						O.free();
					}
				};
			})
		)
	})
