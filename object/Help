An.object().named('Help').from(Image).
	does('down', function(E) {
		if (!E.on(this) || this.blurb) return;
		Sound.click.play();
		this.has('blurb',an(Image,'images/help.png').at(300,0).does('down',function(E) { 
			if(!E.on(this)) return;
			Sound.click().play();
			Phosphor.help().blurb() = false;
			this.free();
		}))
	}).
	does('show', function() { 
		this.down(this); 
	})

